<config>
    <modules>
        <Innoexts_Core>
            <version>1.2.0.0</version>
        </Innoexts_Core>
    </modules>
    <adminhtml>
        <translate>
            <modules>
                <Innoexts_Core>
                    <files>
                        <default>Innoexts_Core.csv</default>
                    </files>
                </Innoexts_Core>
            </modules>
        </translate>
    </adminhtml>
    <global>
        <models>
            <innoexts_core>
                <class>Innoexts_Core_Model</class>
                <resourceModel>innoexts_core_mysql4</resourceModel>
            </innoexts_core>
            <innoexts_core_mysql4>
                <class>Innoexts_Core_Model_Mysql4</class>
            </innoexts_core_mysql4>
        </models>
        <blocks>
            <innoexts_core>
                <class>Innoexts_Core_Block</class>
            </innoexts_core>
        </blocks>
        <helpers>
            <innoexts_core>
                <class>Innoexts_Core_Helper</class>
            </innoexts_core>
        </helpers>
    </global>
</config>